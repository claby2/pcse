INPUT cr
INPUT c
r = 0
IF cr == "THB" THEN
    r = c*26
ELSE IF cr == "GBP" THEN
    r = c*0.47
ELSE
    r = c*0.86
ENDIF
IF 0.02*c > 5 THEN
    OUTPUT r + " " + cr
    OUTPUT "SGD to "+cr
    OUTPUT 1.02*c + " SGD"
ELSE
    OUTPUT r + " " + cr
    OUTPUT "SGD to "+cr
    OUTPUT (c + 5) + " SGD"
ENDIF
